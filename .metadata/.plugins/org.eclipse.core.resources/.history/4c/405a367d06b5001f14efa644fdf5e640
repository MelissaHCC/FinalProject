<!-------Current Silent Auctions End------->
<script>
        // Fetch auction items from the API and dynamically populate the page
        async function loadAuctions() {
            try {
                const response = await fetch('/products');
                if (!response.ok) throw new Error('Failed to fetch auctions');

                const products = await response.json();
                const auctionItemsDiv = document.getElementById('auction-items');
                auctionItemsDiv.innerHTML = ''; // Clear loading message

                if (products.length === 0) {
                    auctionItemsDiv.innerHTML = '<p>No auctions are available at the moment.</p>';
                } else {
                	const imageMap = {
                			1: 'images/auction1.jpg',
                			2: 'images/auction2.jpg',
                			3: 'images/auction3.jpg'
                	};
                    products.forEach(product => {
                        const auctionDiv = document.createElement('div');
                        auctionDiv.className = 'col-md-4 mb-4';

                        auctionDiv.innerHTML = `
                            <div class="auction-item card">
                        	<img src="${imageSrc}" class="card-img-top" alt="${product.name}">
                                <div class="card-body">
                                    <h5 class="card-title">${product.name}</h5>
                                    <p class="card-text">Starting Bid: $${product.price.toFixed(2)}</p>
                                    <a href="#" class="btn btn-primary">View Details & Bid</a>
                                </div>
                            </div>
                        `;
                        auctionItemsDiv.appendChild(auctionDiv);
                    });
                }
            } catch (error) {
                console.error('Error loading auctions:', error);
                document.getElementById('auction-items').innerHTML = '<p>Failed to load auctions.</p>';
            }
        }

        // Load auctions on page load
        document.addEventListener('DOMContentLoaded', loadAuctions);
    </script>

